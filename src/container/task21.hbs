<div style='margin: 30px'>
  <div class='card'>
    <div class='card-body'>
      <div class='card'>
        <div class='card-body'>
          <h1>{{heading.main.title}}</h1>
          <p>{{heading.main.text}}</p>
          <button
            class='btn btn-primary'
          >{{heading.main.button_text.solid}}</button>
          <br />
          <br />
          <button
            class='btn btn-outline-primary'
          >{{heading.main.button_text.outline}}</button>
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          <h2>{{heading.second.title}}</h2>
          <p>{{heading.second.text}}</p>
          <button
            class='btn btn-secondary'
          >{{heading.second.button_text.solid}}</button>
          <br />
          <br />
          <button
            class='btn btn-outline-secondary'
          >{{heading.second.button_text.outline}}</button>
        </div>
      </div>
      <br />
      <div class='card'>
        <div class='card-body'>
          <h3>{{heading.subblock.title}}</h3>
          <p>{{heading.subblock.text}}</p>
          <button
            class='btn btn-dark'
          >{{heading.subblock.button_text.solid}}</button>
          <br />
          <br />
          <button
            class='btn btn-outline-dark'
          >{{heading.subblock.button_text.outline}}</button>
        </div>
      </div>
    </div>
  </div>
  <br />
  <div class='card'>
    <div class='card-body'>
      <h1 class='display-1'>{{nested_display.title}}</h1>
      <br />
      <div class='card'>
        <div class='card-body'>
          <h4
            class='display-4'
          >{{nested_display.children_display.title}}</h4>
          <p>{{nested_display.children_display.text}}</p>
          <button
            class='btn btn-outline-warning'
          >{{nested_display.children_display.button.text}}</button>
        </div>
      </div>
      <br />
      <h6>{{nested_display.header_text}}</h6>
      <br />
      <button
        class='btn btn-info'
      >{{nested_display.button.text}}</button>
    </div>
  </div>
  <br />
  <div class='card'>
    <div class='card-body'>
      <p class='text-uppercase'>{{paragraph.title}}</p>
      <p class='text-lowercase'>{{paragraph.text}}</p>
      <button
        class='btn btn-link'
      >{{paragraph.button.text}}</button>
      <br />
      <br />
      <div class='card'>
        <div class='card-body'>
          <ul class='btn-link'>
            {{#each paragraph.list_name}}
              <li style="margin: 15px"><button class="btn btn-outline-light"><a
                  href='{{this.url}}'
                >{{this.text}}</a></button></li>
            {{/each}}
          </ul>
    </div>
  </div>
</div>